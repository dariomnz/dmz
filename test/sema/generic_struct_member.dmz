// RUN: compiler %s -res-dump 2>&1 | filecheck %s

extern fn printf(fmt:*u8, ...) -> i32;

struct Point<T> {
    x:T,
    y:T,

    fn add(n: T) -> void {
        .x = .x + n;
        .y = .y + n;
    }
// CHECK: ResolvedSpecializedStructDecl Point<i32>
// CHECK-NEXT:   ResolvedFieldDecl:i32 x
// CHECK-NEXT:   ResolvedFieldDecl:i32 y
// CHECK-NEXT:   ResolvedMemberFunctionDecl add -> void
// CHECK-NEXT:     ResolvedParamDecl:*Point 
// CHECK-NEXT:     ResolvedParamDecl:i32 n
// CHECK-NEXT:     ResolvedBlock
// CHECK-NEXT:       ResolvedAssignment
// CHECK-NEXT:         ResolvedSelfMemberExpr:i32 x
// CHECK-NEXT:           ResolvedDeclRefExpr:*Point 
// CHECK-NEXT:         ResolvedBinaryOperator:i32 '+'
// CHECK-NEXT:           ResolvedSelfMemberExpr:i32 x
// CHECK-NEXT:             ResolvedDeclRefExpr:*Point 
// CHECK-NEXT:           ResolvedDeclRefExpr:i32 n
// CHECK-NEXT:       ResolvedAssignment
// CHECK-NEXT:         ResolvedSelfMemberExpr:i32 y
// CHECK-NEXT:           ResolvedDeclRefExpr:*Point 
// CHECK-NEXT:         ResolvedBinaryOperator:i32 '+'
// CHECK-NEXT:           ResolvedSelfMemberExpr:i32 y
// CHECK-NEXT:             ResolvedDeclRefExpr:*Point 
// CHECK-NEXT:           ResolvedDeclRefExpr:i32 n
}

fn main() -> void {
    let p = Point<i32>{x:1, y:2};
    p.add(5);
    printf("Point %d %d\n", p.x, p.y);
}