// RUN: compiler %s -res-dump 2>&1 | filecheck %s
// RUN: diff <(compiler %s -run 2>&1) <(echo -n -e '5\n4\n1\n3\n2\n1\nSecond-last\nThe last one\n')

fn foo(x: i32) -> void {
    defer dprintf(1, "1\n");
    if (x == 1) {
        defer dprintf(1, "2\n");
        defer dprintf(1, "3\n");
        return;
    }else{
        defer dprintf(1, "4\n");
        defer dprintf(1, "5\n");
    }
    return;
}
