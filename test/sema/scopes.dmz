// RUN: compiler %s -res-dump 2>&1 | filecheck %s
extern fn printf(fmt:*u8, ...) -> i32; fn println(n:i32) -> void {printf("%d\n", n);}
fn main() -> void {
    let x = 1;
    println(x);
    {
        let y = 2;
        println(y);
    }
    println(x);
}

// CHECK: ResolvedFunctionDecl main -> Void void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedDeclStmt
// CHECK-NEXT:       ResolvedVarDecl:Int i32 x
// CHECK-NEXT:         ResolvedIntLiteral:Int i32 '1'
// CHECK-NEXT:         | value: 1
// CHECK-NEXT:     ResolvedCallExpr:Void void
// CHECK-NEXT:       ResolvedFunctionDecl println -> Void void
// CHECK-NEXT:       ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:     ResolvedBlock
// CHECK-NEXT:       ResolvedDeclStmt
// CHECK-NEXT:         ResolvedVarDecl:Int i32 y
// CHECK-NEXT:           ResolvedIntLiteral:Int i32 '2'
// CHECK-NEXT:           | value: 2
// CHECK-NEXT:       ResolvedCallExpr:Void void
// CHECK-NEXT:         ResolvedFunctionDecl println -> Void void
// CHECK-NEXT:         ResolvedDeclRefExpr:Int i32 y
// CHECK-NEXT:     ResolvedCallExpr:Void void
// CHECK-NEXT:       ResolvedFunctionDecl println -> Void void
// CHECK-NEXT:       ResolvedDeclRefExpr:Int i32 x