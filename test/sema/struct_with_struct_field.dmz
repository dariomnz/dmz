// RUN: dmz %s -res-dump 2>&1 | filecheck %s
struct Point {
  x: i32,
  y: i32,
}
// CHECK: ResolvedStructDecl  struct_with_struct_field.Point
// CHECK-NEXT:   ResolvedFieldDecl:i32 x
// CHECK-NEXT:   ResolvedFieldDecl:i32 y

struct Color {
  r: i32,
  g: i32,
  b: i32,
  p: Point,
}
// CHECK: ResolvedStructDecl  struct_with_struct_field.Color
// CHECK-NEXT:   ResolvedFieldDecl:i32 r
// CHECK-NEXT:   ResolvedFieldDecl:i32 g
// CHECK-NEXT:   ResolvedFieldDecl:i32 b
// CHECK-NEXT:   ResolvedFieldDecl:struct_with_struct_field.Point{} p

fn main() -> void {
  let x = Color { 
    r: 0,
    g: 1,
    b: 2,
    p: Point { x: 3 + 4, y: 4 * 9 },
  };
}
// CHECK: ResolvedFunctionDecl main fn()->void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedDeclStmt
// CHECK-NEXT:       ResolvedVarDecl:struct_with_struct_field.Color{} x
// CHECK-NEXT:         ResolvedStructInstantiationExpr:struct_with_struct_field.Color{}
// CHECK-NEXT:           ResolvedFieldInitStmt:i32 r
// CHECK-NEXT:             ResolvedIntLiteral:i32 '0'
// CHECK-NEXT:             | value: 0
// CHECK-NEXT:           ResolvedFieldInitStmt:i32 g
// CHECK-NEXT:             ResolvedIntLiteral:i32 '1'
// CHECK-NEXT:             | value: 1
// CHECK-NEXT:           ResolvedFieldInitStmt:i32 b
// CHECK-NEXT:             ResolvedIntLiteral:i32 '2'
// CHECK-NEXT:             | value: 2
// CHECK-NEXT:           ResolvedFieldInitStmt:struct_with_struct_field.Point{} p
// CHECK-NEXT:             ResolvedStructInstantiationExpr:struct_with_struct_field.Point{}
// CHECK-NEXT:               ResolvedFieldInitStmt:i32 x
// CHECK-NEXT:                 ResolvedBinaryOperator:i32 '+'
// CHECK-NEXT:                 | value: 7
// CHECK-NEXT:                   ResolvedIntLiteral:i32 '3'
// CHECK-NEXT:                   ResolvedIntLiteral:i32 '4'
// CHECK-NEXT:               ResolvedFieldInitStmt:i32 y
// CHECK-NEXT:                 ResolvedBinaryOperator:i32 '*'
// CHECK-NEXT:                 | value: 36
// CHECK-NEXT:                   ResolvedIntLiteral:i32 '4'
// CHECK-NEXT:                   ResolvedIntLiteral:i32 '9'