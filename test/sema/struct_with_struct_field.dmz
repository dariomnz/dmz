// RUN: compiler %s -res-dump 2>&1 | filecheck %s
struct Point {
  x: i32,
  y: i32,
}
// CHECK: ResolvedStructDecl Struct Point
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 x
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 y

struct Color {
  r: i32,
  g: i32,
  b: i32,
  p: Point,
}
// CHECK: ResolvedStructDecl Struct Color
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 r
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 g
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 b
// CHECK-NEXT:   ResolvedFieldDecl:Struct Point p

fn main() -> void {
  let x = Color { 
    r: 0,
    g: 1,
    b: 2,
    p: Point { x: 3 + 4, y: 4 * 9 },
  };
}
// CHECK: ResolvedFunctionDecl main -> Void void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedDeclStmt
// CHECK-NEXT:       ResolvedVarDecl:Struct Color x
// CHECK-NEXT:         ResolvedStructInstantiationExpr:Struct Color
// CHECK-NEXT:           ResolvedFieldInitStmt:Int i32 r
// CHECK-NEXT:             ResolvedIntLiteral:Int i32 '0'
// CHECK-NEXT:             | value: 0
// CHECK-NEXT:           ResolvedFieldInitStmt:Int i32 g
// CHECK-NEXT:             ResolvedIntLiteral:Int i32 '1'
// CHECK-NEXT:             | value: 1
// CHECK-NEXT:           ResolvedFieldInitStmt:Int i32 b
// CHECK-NEXT:             ResolvedIntLiteral:Int i32 '2'
// CHECK-NEXT:             | value: 2
// CHECK-NEXT:           ResolvedFieldInitStmt:Struct Point p
// CHECK-NEXT:             ResolvedStructInstantiationExpr:Struct Point
// CHECK-NEXT:               ResolvedFieldInitStmt:Int i32 x
// CHECK-NEXT:                 ResolvedBinaryOperator:Int i32 '+'
// CHECK-NEXT:                 | value: 7
// CHECK-NEXT:                   ResolvedIntLiteral:Int i32 '3'
// CHECK-NEXT:                   ResolvedIntLiteral:Int i32 '4'
// CHECK-NEXT:               ResolvedFieldInitStmt:Int i32 y
// CHECK-NEXT:                 ResolvedBinaryOperator:Int i32 '*'
// CHECK-NEXT:                 | value: 36
// CHECK-NEXT:                   ResolvedIntLiteral:Int i32 '4'
// CHECK-NEXT:                   ResolvedIntLiteral:Int i32 '9'