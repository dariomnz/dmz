// RUN: compiler %s -res-dump 2>&1 | filecheck %s

extern fn printf(fmt:*u8, ...) -> i32;

fn main() -> void {
    let x = 10;

    switch(x) {
        case 1 => printf("1\n");
        case 2 => printf("2\n");
        case 3 => printf("3\n");
        else   => printf("else %d\n", x);
    }
}
// CHECK: ResolvedSwitchStmt
// CHECK-NEXT:      ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:      ResolvedCaseStmt
// CHECK-NEXT:        ResolvedIntLiteral:Int i32 '1'
// CHECK-NEXT:        | value: 1
