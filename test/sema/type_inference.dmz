// RUN: compiler %s -res-dump 2>&1 | filecheck %s --implicit-check-not error --match-full-lines
fn foo() -> int {
    return 1;
}

fn bar() -> char {
    return '\n';
}

fn str() -> char[] {
    return "hello\n";
}

fn main() -> void {
    let x = 1;
    let y = foo() + 1;

    let x2: int;
    let y2: int;

    let c1 = 'a';
    let c2 = bar();
    let c3: char;

    let str1 = "something\n";
    let str2 = str();
}

// CHECK:   ResolvedVarDecl: x:int
// CHECK-NEXT:     ResolvedIntLiteral: '1'
// CHECK-NEXT:     | value: 1
// CHECK-NEXT: ResolvedDeclStmt:
// CHECK-NEXT:   ResolvedVarDecl: y:int
// CHECK-NEXT:     ResolvedBinaryOperator: '+'
// CHECK-NEXT:       ResolvedCallExpr: foo
// CHECK-NEXT:       ResolvedIntLiteral: '1'

// CHECK:      ResolvedVarDecl: str1:char[11]
// CHECK-NEXT:        ResolvedStringLiteral: 'something\n'
// CHECK-NEXT:    ResolvedDeclStmt:
// CHECK-NEXT:      ResolvedVarDecl: str2:char[]
// CHECK-NEXT:        ResolvedCallExpr: str