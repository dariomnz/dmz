// RUN: compiler %s -res-dump 2>&1 | filecheck %s --implicit-check-not error --match-full-lines
fn foo() -> int {
    return 1;
}

fn bar() -> char {
    return '\n';
}

fn str() -> char[] {
    return "hello\n";
}

fn fnBool() -> bool {
    return true;
}

fn main() -> void {
    let x = 1;
    let y = foo() + 1;

    let x2: int;
    let y2: int;

    let c1 = 'a';
    let c2 = bar();
    let c3: char;

    let str1 = "something\n";
    let str2 = str();

    let bool1 = true;
    let bool2 = false;
    let bool3 = fnBool();
}

// CHECK:   ResolvedVarDecl:int x
// CHECK-NEXT:     ResolvedIntLiteral:int '1'
// CHECK-NEXT:     | value:int 1
// CHECK-NEXT: ResolvedDeclStmt
// CHECK-NEXT:   ResolvedVarDecl:int y
// CHECK-NEXT:     ResolvedBinaryOperator:int '+'
// CHECK-NEXT:       ResolvedCallExpr:int foo
// CHECK-NEXT:       ResolvedIntLiteral:int '1'

// CHECK:      ResolvedVarDecl:char[11] str1
// CHECK-NEXT:        ResolvedStringLiteral:char[11] 'something\n'
// CHECK-NEXT:    ResolvedDeclStmt
// CHECK-NEXT:      ResolvedVarDecl:char[] str2
// CHECK-NEXT:        ResolvedCallExpr:char[] str

// CHECK:   ResolvedVarDecl:bool bool1
// CHECK-NEXT:     ResolvedBoolLiteral:bool 'true'
// CHECK-NEXT:     | value:bool true
// CHECK-NEXT: ResolvedDeclStmt
// CHECK-NEXT:   ResolvedVarDecl:bool bool2
// CHECK-NEXT:     ResolvedBoolLiteral:bool 'false'
// CHECK-NEXT:     | value:bool false
// CHECK-NEXT: ResolvedDeclStmt
// CHECK-NEXT:   ResolvedVarDecl:bool bool3
// CHECK-NEXT:     ResolvedCallExpr:bool fnBool