// RUN: dmz %s -res-dump 2>&1 | filecheck %s
struct Color {
  r: i32,
  g: i32,
  b: i32,
}

fn main() -> void {
  // CHECK: [[# @LINE + 3 ]]:5: error: 'Color' has no field named 'rgba'
  let x = Color {
    r: 0,
    rgba: 1,
    g: 2,
    b: 3,
    a: 4,
  };
  // CHECK: [[# @LINE - 2 ]]:5: error: 'Color' has no field named 'a'
}
