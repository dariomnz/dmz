// RUN: compiler %s -res-dump 2>&1 | filecheck %s

extern fn foo() -> void;
// CHECK: ResolvedExternFunctionDecl foo -> void

extern fn bar(a:int) -> void;
// CHECK: ResolvedExternFunctionDecl bar -> void
// CHECK-NEXT:  ResolvedParamDecl:int a

extern fn dprintf(fd:int, format: &char[], ...) -> int;
// CHECK: ResolvedExternFunctionDecl dprintf -> int
// CHECK-NEXT:  ResolvedParamDecl:int fd
// CHECK-NEXT:  ResolvedParamDecl:&char[] format
// CHECK-NEXT:  ResolvedParamDecl:vararg ...

fn main() -> void {
    let i = 10;
    //let i_ref = &i;
    dprintf(1, &"Hello %d %d %d\n", 1, 2, 3);
}