// RUN: compiler %s -res-dump 2>&1 | filecheck %s
struct Color {
  r: i32,
  g: i32,
  b: i32,
}
// CHECK: ResolvedStructDecl Struct Color
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 r
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 g
// CHECK-NEXT:   ResolvedFieldDecl:Int i32 b

fn main() -> void {
  let x = Color { 
    r: 0,
    g: 1,
    b: 2,
  };
}
// CHECK: ResolvedFunctionDecl main -> Void void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedDeclStmt
// CHECK-NEXT:       ResolvedVarDecl:Struct Color x
// CHECK-NEXT:         ResolvedStructInstantiationExpr:Struct Color
// CHECK-NEXT:           ResolvedFieldInitStmt:Int i32 r
// CHECK-NEXT:             ResolvedIntLiteral:Int i32 '0'
// CHECK-NEXT:             | value: 0
// CHECK-NEXT:           ResolvedFieldInitStmt:Int i32 g
// CHECK-NEXT:             ResolvedIntLiteral:Int i32 '1'
// CHECK-NEXT:             | value: 1
// CHECK-NEXT:           ResolvedFieldInitStmt:Int i32 b
// CHECK-NEXT:             ResolvedIntLiteral:Int i32 '2'
// CHECK-NEXT:             | value: 2
