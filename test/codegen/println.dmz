// RUN: compiler %s -llvm-dump 2>&1 | filecheck %s
// RUN: diff <(compiler %s -run) <(echo -n -e '0\n10\n12345\n10002345\n123456789\n')
extern fn printf(fmt:&char[], ...) -> int; fn println(const n:int) -> void {printf(&"%d\n", n);}
fn main() -> void {
    println(0);
    println(10);
    println(12345);
    println(10002345);
    println(123456789);
}
// CHECK: declare i32 @printf(ptr byref(ptr) %0, ...)

