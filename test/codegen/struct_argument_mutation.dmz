// RUN: diff <(compiler %s -run 2>&1) <(echo -n -e '6\n8\n1\n2\n')
struct S {
  x: i32,
  y: i32,
}

fn foo(s: S) -> void {
  let aux = s;
  aux.x = 6;
  println(aux.x);
}

fn bar(s: S) -> S {
  let aux = s;
  aux.y = 8;
  return aux;
}
extern fn printf(fmt:*u8, ...) -> i32; fn println(n:i32) -> void {printf(&"%d\n", n);}

fn main() -> void {
  let s = S { x: 1, y: 2 };
  foo(s);
  println(bar(s).y);

  println(s.x);
  println(s.y);
}
