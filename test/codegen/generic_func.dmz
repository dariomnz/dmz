// RUN: compiler %s -llvm-dump 2>&1 | filecheck %s
// RUN: diff <(compiler %s -run 2>&1) <(echo -n -e 'One integer 1\nOne integer 2\nIntegers 123 1234 12345\n')

extern fn printf(fmt:*u8, ...) -> i32;

fn print<T>(const param:T) -> void {
    printf("One integer %d\n", param);
}

// CHECK: define void @print__gen__i32(i32 %param) {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %0 = call i32 (ptr, ...) @printf(ptr @global.str, i32 %param)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }

// CHECK: define void @print__gen__i64(i64 %param) {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %0 = call i32 (ptr, ...) @printf(ptr @global.str.1, i64 %param)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }

fn foo<T, F, G>(const param1:T, const param2:F, const param3:G) -> void {
    printf("Integers %d %d %d\n", param1, param2, param3);
}

// CHECK: define void @foo__gen__i16__i32__i64(i16 %param1, i32 %param2, i64 %param3) {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %0 = call i32 (ptr, ...) @printf(ptr @global.str.2, i16 %param1, i32 %param2, i64 %param3)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }

fn main() -> void {
    print<i32>(1);
    print<i64>(2);
    foo<i16, i32, i64>(123, 1234, 12345);
}

// CHECK: define void @__builtin_main() {
// CHECK-NEXT: entry:
// CHECK-NEXT:   call void @print__gen__i32(i32 1)
// CHECK-NEXT:   call void @print__gen__i64(i32 2)
// CHECK-NEXT:   call void @foo__gen__i16__i32__i64(i32 123, i32 1234, i32 12345)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }