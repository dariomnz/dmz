// RUN: diff <(dmz %s -run 2>&1) <(echo -n -e '1\n-1\n-9\n')
extern fn printf(fmt:*u8, ...) -> i32; fn println(n:i32) -> void {printf("%d\n", n);}
fn foo(x: i32) -> i32 {
    if (x < (0 - 5)) {
        return (0 - 1);
    } else if (x > 5) {
        return 1;
    }

    const x1: i32 = 2 + 3;
    
    let y: i32 = !x1;
    
    y = 3 * !y / (1 + 2);

    return y - 10;
}

fn main() -> void {
    println(foo(6));
    println(foo(0 - 6));
    println(foo(1));
}
