extern fn printf(fmt:&char[], ...) -> int; fn println(const n:int) -> void {printf(&"%d\n", n);}
fn fib(n: int) -> int {
    if (n == 0 || n == 1) {
        return n;
    }

    return fib(n - 1) + fib(n - 2);
}

fn main() -> void {
    let i = 0;
    while (i < 20) {
        println(fib(i));
        i = i + 1;
    }
}
