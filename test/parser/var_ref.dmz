// RUN: dmz %s -ast-dump 2>&1 | filecheck %s

fn foo(x: *i32) -> void {
    *x = 20;
}
// CHECK: FunctionDecl foo -> void
// CHECK-NEXT:   ParamDecl:*i32 x
// CHECK-NEXT:   Block

fn main() -> void {
    let i = 10;
    foo(&i);
}

// CHECK:      CallExpr
// CHECK-NEXT:   DeclRefExpr foo
// CHECK-NEXT:   RefPtrExpr
// CHECK-NEXT:     DeclRefExpr i