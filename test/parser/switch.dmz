// RUN: dmz %s -ast-dump 2>&1 | filecheck %s
fn main() -> void {
    let x = 10;

    switch(x) {
        case 1 => printf(&"1\n");
        case 2 => printf(&"2\n");
        case 3 => printf(&"3\n");
        else   => printf(&"else %d\n", x);
    }
}

