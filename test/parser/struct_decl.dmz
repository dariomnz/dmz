// RUN: compiler %s -ast-dump 2>&1 | filecheck %s
struct Empty {}
// CHECK: StructDecl Empty

struct WithTrailingComma {
    r: i32,
    g: i32,
    b: i32,
}
// CHECK: StructDecl WithTrailingComma
// CHECK-NEXT:   FieldDecl:Int i32 r
// CHECK-NEXT:   FieldDecl:Int i32 g
// CHECK-NEXT:   FieldDecl:Int i32 b

struct WithoutTrailingComma {
    a: i32,
    b: i32,
    c: i32
}
// CHECK: StructDecl WithoutTrailingComma
// CHECK-NEXT:   FieldDecl:Int i32 a
// CHECK-NEXT:   FieldDecl:Int i32 b
// CHECK-NEXT:   FieldDecl:Int i32 c
