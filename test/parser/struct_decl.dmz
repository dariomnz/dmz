// RUN: compiler %s -ast-dump 2>&1 | filecheck %s
struct Empty {}
// CHECK: StructDecl: Empty

struct WithTrailingComma {
    r: int,
    g: int,
    b: int,
}
// CHECK: StructDecl: WithTrailingComma
// CHECK-NEXT:   FieldDecl: r:int
// CHECK-NEXT:   FieldDecl: g:int
// CHECK-NEXT:   FieldDecl: b:int

struct WithoutTrailingComma {
    a: int,
    b: int,
    c: int
}
// CHECK: StructDecl: WithoutTrailingComma
// CHECK-NEXT:   FieldDecl: a:int
// CHECK-NEXT:   FieldDecl: b:int
// CHECK-NEXT:   FieldDecl: c:int
