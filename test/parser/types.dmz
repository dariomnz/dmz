// RUN: compiler %s -ast-dump 2>&1 | filecheck %s
fn main() -> void {
    let i: int = 10;
    // CHECK: VarDecl:int i
    // CHECK-NEXT:   IntLiteral '10'

    let c1: char = 'c';
    // CHECK: VarDecl:char c1
    // CHECK-NEXT:   CharLiteral ''c''

    let c2: char = '\n';
    // CHECK: VarDecl:char c2
    // CHECK-NEXT:   CharLiteral ''\n''

    let c3: char[] = "Hello, world\n";
    // CHECK: VarDecl:char[] c3
    // CHECK-NEXT:   StringLiteral '"Hello, world\n"'

    let b1: bool = true;
    // CHECK: VarDecl:bool b1
    // CHECK-NEXT:   BoolLiteral 'true'

    let b2: bool = false;
    // CHECK: VarDecl:bool b2
    // CHECK-NEXT:   BoolLiteral 'false'
}
