// RUN: dmz %s -ast-dump -module 2>&1 | filecheck %s

fn add(x:i32, y:i32) -> i32 {
    return x + y;
}

test "add test" {
    try std.testing.expect(add(1, 1) == 2);
    try std.testing.expect(add(1, 1) == 3);
}
