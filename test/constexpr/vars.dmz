// RUN: compiler %s -res-dump -no-remove-unused 2>&1 | filecheck %s --match-full-lines
fn pass() -> i32 {
    const x: i32 = 2;
    return x * 10;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedBinaryOperator:Int i32 '*'
// CHECK-NEXT:       | value: 20
// CHECK-NEXT:         ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:       | value: 2
// CHECK-NEXT:         ResolvedIntLiteral:i32 '10'

fn pass2() -> i32 {
    const x: i32 = 2;
    return x;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:       | value: 2

fn error1() -> i32 {
    const x: i32;
    x = 2;
    return x * 10;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedBinaryOperator:Int i32 '*'
// CHECK-NEXT:         ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:         ResolvedIntLiteral:i32 '10'

fn err2() -> i32 {
    let x: i32 = 2;
    return x * 10;
}
// CHECK:    ResolvedReturnStmt
// CHECK-NEXT:      ResolvedBinaryOperator:Int i32 '*'
// CHECK-NEXT:        ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:        ResolvedIntLiteral:i32 '10'

fn main() -> void {}
