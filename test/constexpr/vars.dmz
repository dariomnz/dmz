// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn pass() -> int {
    let const x: int = 2;
    return x * 10;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedBinaryOperator:int '*'
// CHECK-NEXT:       | value:int 20
// CHECK-NEXT:         ResolvedDeclRefExpr:int x
// CHECK-NEXT:         ResolvedIntLiteral:int '10'

fn pass2() -> int {
    let const x: int = 2;
    return x;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedDeclRefExpr:int x
// CHECK-NEXT:       | value:int 2

fn error() -> int {
    let const x: int;
    x = 2;
    return x * 10;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedBinaryOperator:int '*'
// CHECK-NEXT:         ResolvedDeclRefExpr:int x
// CHECK-NEXT:         ResolvedIntLiteral:int '10'

fn err2() -> int {
    let x: int = 2;
    return x * 10;
}
// CHECK:    ResolvedReturnStmt
// CHECK-NEXT:      ResolvedBinaryOperator:int '*'
// CHECK-NEXT:        ResolvedDeclRefExpr:int x
// CHECK-NEXT:        ResolvedIntLiteral:int '10'

fn main() -> void {}
