// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn foo(x: int) -> void {}

fn bar(x: int) -> void {
    foo(x);
}

fn main() -> void {
    bar(1);
}

// CHECK: ResolvedFunctionDecl: foo -> void
// CHECK-NEXT:   ResolvedParamDecl: x:int
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT: ResolvedFunctionDecl: bar -> void
// CHECK-NEXT:   ResolvedParamDecl: x:int
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedDeclRefExpr: x:int
// CHECK-NEXT: ResolvedFunctionDecl: main -> void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: bar:void
// CHECK-NEXT:       ResolvedIntLiteral: '1'
// CHECK-NEXT:       | value: 1
