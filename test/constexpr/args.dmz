// RUN: dmz %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn foo(x: i32) -> void {}

fn bar(x: i32) -> void {
    foo(x);
}

fn main() -> void {
    bar(1);
}

// CHECK: ResolvedFunctionDecl foo fn(i32)->void
// CHECK-NEXT:   ResolvedParamDecl:i32 x
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT: ResolvedFunctionDecl bar fn(i32)->void
// CHECK-NEXT:   ResolvedParamDecl:i32 x
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr:void 
// CHECK-NEXT:       ResolvedDeclRefExpr:fn(i32)->void foo
// CHECK-NEXT:       ResolvedDeclRefExpr:i32 x
// CHECK-NEXT: ResolvedFunctionDecl main fn()->void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr:void 
// CHECK-NEXT:       ResolvedDeclRefExpr:fn(i32)->void bar
// CHECK-NEXT:       ResolvedIntLiteral:i32 '1'
// CHECK-NEXT:       | value: 1
