// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn foo(x: int) -> void {}

fn bar(x: int) -> void {
    foo(x);
}

fn main() -> void {
    bar(1);
}

// CHECK: ResolvedFunctionDecl foo -> void
// CHECK-NEXT:   ResolvedParamDecl:int x
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT: ResolvedFunctionDecl bar -> void
// CHECK-NEXT:   ResolvedParamDecl:int x
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr:void foo
// CHECK-NEXT:       ResolvedDeclRefExpr:int x
// CHECK-NEXT: ResolvedFunctionDecl main -> void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr:void bar
// CHECK-NEXT:       ResolvedIntLiteral:int '1'
// CHECK-NEXT:       | value:int 1
