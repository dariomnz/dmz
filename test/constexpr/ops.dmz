// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn foo(n: int) -> void {}

fn prefix(x: int) -> void {
    foo(!1);
    foo(!0);
    foo(!x);
    foo(-x);
    foo(-1);
}
// CHECK: ResolvedFunctionDecl: prefix -> void
// CHECK-NEXT:   ResolvedParamDecl: x:int
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedUnaryOperator: '!'
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral: '1'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedUnaryOperator: '!'
// CHECK-NEXT:       | value: 1
// CHECK-NEXT:         ResolvedIntLiteral: '0'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedUnaryOperator: '!'
// CHECK-NEXT:         ResolvedDeclRefExpr: x:int
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedUnaryOperator: '-'
// CHECK-NEXT:         ResolvedDeclRefExpr: x:int
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedUnaryOperator: '-'
// CHECK-NEXT:       | value: -1
// CHECK-NEXT:         ResolvedIntLiteral: '1'

fn multiplicative(x: int) -> void {
    foo(5 * 3);
    foo(20 / 4);

    foo(x * 1);
    foo(1 * x);
}
// CHECK: ResolvedFunctionDecl: multiplicative -> void
// CHECK-NEXT:   ResolvedParamDecl: x:int
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '*'
// CHECK-NEXT:       | value: 15
// CHECK-NEXT:         ResolvedIntLiteral: '5'
// CHECK-NEXT:         ResolvedIntLiteral: '3'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '/'
// CHECK-NEXT:       | value: 5
// CHECK-NEXT:         ResolvedIntLiteral: '20'
// CHECK-NEXT:         ResolvedIntLiteral: '4'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '*'
// CHECK-NEXT:         ResolvedDeclRefExpr: x:int
// CHECK-NEXT:         ResolvedIntLiteral: '1'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '*'
// CHECK-NEXT:         ResolvedIntLiteral: '1'
// CHECK-NEXT:         ResolvedDeclRefExpr: x:int

fn additive() -> void {
    foo(5 + 3);
    foo(20 - 4);
}
// CHECK: ResolvedFunctionDecl: additive -> void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '+'
// CHECK-NEXT:       | value: 8
// CHECK-NEXT:         ResolvedIntLiteral: '5'
// CHECK-NEXT:         ResolvedIntLiteral: '3'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '-'
// CHECK-NEXT:       | value: 16
// CHECK-NEXT:         ResolvedIntLiteral: '20'
// CHECK-NEXT:         ResolvedIntLiteral: '4'

fn comparison() -> void {
    foo(2 < 5);
    foo(5 < 2);

    foo(2 > 5);
    foo(5 > 2);
}
// CHECK: ResolvedFunctionDecl: comparison -> void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '<'
// CHECK-NEXT:       | value: 1
// CHECK-NEXT:         ResolvedIntLiteral: '2'
// CHECK-NEXT:         ResolvedIntLiteral: '5'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '<'
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral: '5'
// CHECK-NEXT:         ResolvedIntLiteral: '2'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '>'
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral: '2'
// CHECK-NEXT:         ResolvedIntLiteral: '5'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '>'
// CHECK-NEXT:       | value: 1
// CHECK-NEXT:         ResolvedIntLiteral: '5'
// CHECK-NEXT:         ResolvedIntLiteral: '2'

fn equality() -> void {
    foo(2 == 2);
    foo(5 == 3);
}
// CHECK: ResolvedFunctionDecl: equality -> void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '=='
// CHECK-NEXT:       | value: 1
// CHECK-NEXT:         ResolvedIntLiteral: '2'
// CHECK-NEXT:         ResolvedIntLiteral: '2'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '=='
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral: '5'
// CHECK-NEXT:         ResolvedIntLiteral: '3'

fn conjunction() -> void {
    foo(2 && 3);
    foo(0 && 1);
    foo(1 && 0);
    foo(0 && 0);
}
// CHECK: ResolvedFunctionDecl: conjunction -> void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '&&'
// CHECK-NEXT:       | value: 1
// CHECK-NEXT:         ResolvedIntLiteral: '2'
// CHECK-NEXT:         ResolvedIntLiteral: '3'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '&&'
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral: '0'
// CHECK-NEXT:         ResolvedIntLiteral: '1'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '&&'
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral: '1'
// CHECK-NEXT:         ResolvedIntLiteral: '0'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '&&'
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral: '0'
// CHECK-NEXT:         ResolvedIntLiteral: '0'

fn disjunction() -> void {
    foo(2 || 0);
    foo(0 || 2);
    foo(2 || 3);
    foo(0 || 0);
}
// CHECK: ResolvedFunctionDecl: disjunction -> void
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr: foo:void
// CHECK-NEXT:       ResolvedBinaryOperator: '||'
