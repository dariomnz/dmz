// CHECK-NEXT:       ResolvedBinaryOperator: '||'
// CHECK-NEXT:       | value: 1
// CHECK-NEXT:         ResolvedIntLiteral: '2'
// CHECK-NEXT:         ResolvedIntLiteral: '3'
// CHECK-NEXT:     ResolvedCallExpr: foo:void
