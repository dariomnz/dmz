// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn foo(n: i32) -> void {}
fn bar(n: i1) -> void {}

fn prefix(x: i32) -> void {
    bar(!1);
    bar(!0);
    bar(!x);
    foo(-x);
    foo(-1);
}
// CHECK: ResolvedFunctionDecl prefix -> Void void
// CHECK-NEXT:   ResolvedParamDecl:Int i32 x
// CHECK-NEXT:   ResolvedBlock
// CHECK-NEXT:     ResolvedCallExpr:Void void
// CHECK-NEXT:       ResolvedFunctionDecl bar -> Void void
// CHECK-NEXT:       ResolvedUnaryOperator:Int i1 '!'
// CHECK-NEXT:       | value: 0
// CHECK-NEXT:         ResolvedIntLiteral:Int i32 '1'
// CHECK-NEXT:     ResolvedCallExpr:Void void
// CHECK-NEXT:       ResolvedFunctionDecl bar -> Void void
// CHECK-NEXT:       ResolvedUnaryOperator:Int i1 '!'
// CHECK-NEXT:       | value: 1
// CHECK-NEXT:         ResolvedIntLiteral:Int i32 '0'
// CHECK-NEXT:     ResolvedCallExpr:Void void
// CHECK-NEXT:       ResolvedFunctionDecl bar -> Void void
// CHECK-NEXT:       ResolvedUnaryOperator:Int i1 '!'
// CHECK-NEXT:         ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:     ResolvedCallExpr:Void void
// CHECK-NEXT:       ResolvedFunctionDecl foo -> Void void
// CHECK-NEXT:       ResolvedUnaryOperator:Int i32 '-'
// CHECK-NEXT:         ResolvedDeclRefExpr:Int i32 x
// CHECK-NEXT:     ResolvedCallExpr:Void void
// CHECK-NEXT:       ResolvedFunctionDecl foo -> Void void
// CHECK-NEXT:       ResolvedUnaryOperator:Int i32 '-'
// CHECK-NEXT:       | value: -1
// CHECK-NEXT:         ResolvedIntLiteral:Int i32 '1'

fn multiplicative(x: i32) -> void {
    foo(5 * 3);
    foo(20 / 4);

    foo(x * 1);
    foo(1 * x);
}
